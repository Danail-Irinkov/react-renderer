{
  "author": {
    "name": "Danail Irinkov",
    "url": "https://github.com/Danail-Irinkov"
  },
  "keywords": [
    "react",
    "reactjs",
    "webpack",
    "react-bundle-renderer",
    "tailwind"
  ],
  "license": "MIT",
  "name": "react-bundle-renderer",
  "version": "1.0.1",
  "description": "Webpack 6 boilerplate with React 17, Tailwind 2, using babel, sass, with HMR and an optimized production build",
  "dependencies": {
    "canvas": "^2.8.0",
    "chart.js": "^3.6.2",
    "chartjs-node-canvas": "^4.1.5",
    "faker": "^5.5.3",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.0.0",
    "react-data-table-component": "^7.4.5",
    "react-dom": "^17.0.2",
    "request": "^2.79.0",
    "styled-components": "^5.3.3",
    "tailwindcss": "^2.2.7"
  },
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "webpack serve --config config/webpack.dev.js",
    "build": "webpack --config config/webpack.bundle.js",
    "prettify": "yarn prettier \"**/*.*(js|jsx)\" --ignore-path=.prettierignore --write",
    "lint": "eslint src --ext .js,.jsx",
    "lint-fix": "eslint src --ext .js,.jsx --fix",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged --verbose"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@svgr/webpack": "^4.3.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "autoprefixer": "^10.3.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "*",
    "copy-webpack-plugin": "^5.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^1.6.2",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^4.3.0",
    "prettier": "^2.5.1",
    "react-refresh": "^0.9.0",
    "react-server-renderer": "^1.2.0",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^3.11.3",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
